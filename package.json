    "leaflet": "^1.9.3",
    "leaflet-rotatedmarker": "^0.2.0",
    "leaflet-timedimension": "^1.1.1",
    "moment": "^2.29.4",
    "pinia": "^2.0.16",
    "register-service-worker": "^1.7.1",
    "sass": "^1.53.0",
    "steelseries": "^2.0.7",
