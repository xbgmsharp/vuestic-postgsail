    "lint": "eslint --fix './src/**/*.{ts,js,vue}'",
    "format": "prettier --write .",
    "tsc": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "cypress run",
    "cy:run": "cypress run"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,vue}": [
      "npm run lint"
    ]
  },
