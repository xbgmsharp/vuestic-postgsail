  "version": "0.1.0-beta12",
  "scripts": {
    "prepare": "husky",
    "dev": "vite",
    "build": "npm run lint && npm run tsc && VITE_CJS_TRACE=true vite build",
    "build:ci": "vite build",
    "build:client": "vite build --outDir dist/client --ssrManifest",
